{
  "name": "uni-design",
  "version": "0.0.3",
  "description": "ui component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "jest --config jest.config.js --verbose=false",
    "build": "gulp && npm run build:es && npm run build:lib",
    "build:es": "tsc -d --emitDeclarationOnly --declarationDir es",
    "build:lib": "tsc -d --emitDeclarationOnly --declarationDir lib",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:style-fix": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "dev": "NODE_ENV=development start-storybook -p 9999 -c .storybook",
    "storybook": "rm -rf ./node_modules/@storybook/react/node_modules/react && rm -rf ./node_modules/@storybook/react/node_modules/react-dom && start-storybook -p 9999"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/components/**/*.tsx": [
      "prettier --write",
      "tslint --config tslint.json --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add"
    ]
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snakeUni/mui-design.git"
  },
  "keywords": [
    "UI",
    "Component",
    "hooks",
    "react"
  ],
  "author": "lanyincao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snakeUni/mui-design/issues"
  },
  "homepage": "https://github.com/snakeUni/mui-design#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-transform-typescript": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@storybook/addon-info": "^4.2.0-alpha.0",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/addon-options": "^4.2.0-alpha.0",
    "@storybook/addon-storysource": "^4.1.4",
    "@storybook/components": "^4.2.0-alpha.0",
    "@storybook/react": "^4.2.0-alpha.0",
    "@types/classnames": "^2.2.6",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/react": "16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-transition-group": "^2.0.15",
    "@types/warning": "^3.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "commitlint": "^7.2.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.11.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-react-hooks": "^1.1.0-alpha.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-typescript": "^5.0.0",
    "husky": "^1.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.4",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-transition-group": "^2.5.3",
    "warning": "^4.0.2"
  },
  "peerDependencies": {
    "react": ">= 16.6.3",
    "react-dom": ">= 16.6.3"
  },
  "resolutions": {
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2"
  }
}
